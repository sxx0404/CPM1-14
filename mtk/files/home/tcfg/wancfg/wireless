
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path '10180000.wmac'
	option htmode 'HT20'
	option disabled '0'
	option channel '6'
	option txpower '20'
	option country '00'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option ssid 'CPM1'
	option encryption 'none'
	option network 'lan'
